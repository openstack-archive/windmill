# This file is generated by Ansible
#  DO NOT EDIT THIS FILE BY HAND -- YOUR CHANGES WILL BE OVERWRITTEN
#
[gearman]
port = 4730
server = {{ hostvars['zs01'].ansible_host }}

[gearman_server]
listen_address = {{ hostvars['zs01'].ansible_host }}
log_config = /etc/zuul/gearman-logging.conf
start = true

[gerrit]
port = 29418
server = 127.0.0.1
sshkey = /var/lib/zuul-test/.ssh/id_rsa
user = zuul

[scheduler]
tenant_config = /etc/zuul/config/layout.yaml
log_config = /etc/zuul/scheduler-logging.conf
pidfile = /var/run/zuul-scheduler/zuul-scheduler.pid
state_dir = /var/lib/zuul-test

[executor]
log_config = /etc/zuul/executor-logging.conf
workspace_root=/var/lib/zuul-test/workspace

[merger]
git_dir = /var/lib/zuul-test/git
log_config = /etc/zuul/merger-logging.conf
pidfile = /var/run/zuul-merger/zuul-merger.pid
zuul_url = {{ hostvars['zs01'].ansible_host }}
